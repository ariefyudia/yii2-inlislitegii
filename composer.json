    "autoload": {
        "psr-4": {
            "inlislite\\gii\\": ""
        }
    },
    "require": {
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "3.5.1",
        "kartik-v/yii2-icons": "*",
        "kartik-v/yii2-datecontrol": "*",
