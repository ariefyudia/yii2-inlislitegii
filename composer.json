    "homepage": "https://github.com/inlislite-ext/yii2-inlislitegii",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Henry",
            "email": "alvin_vna@yahoo.com"
        },
		{
            "name": "Andy Kurniawan",
            "email": "dodot.kurniawan@gmail.com"
        }
